#===============================================================================
# Node Manager Properties
# Generated by WebLogic Domain Automation Phase 2
#
# Location: {{DOMAIN_DOMAIN_HOME}}/nodemanager/nodemanager.properties
#===============================================================================

#-------------------------------------------------------------------------------
# Basic Configuration
#-------------------------------------------------------------------------------
DomainsFile={{DOMAIN_DOMAIN_HOME}}/nodemanager/nodemanager.domains
NodeManagerHome={{DOMAIN_DOMAIN_HOME}}/nodemanager
JavaHome={{DOMAIN_JAVA_HOME}}

#-------------------------------------------------------------------------------
# Network Configuration
#-------------------------------------------------------------------------------
ListenAddress={{NODEMANAGER_LISTEN_ADDRESS}}
ListenPort={{NODEMANAGER_LISTEN_PORT}}
ListenBacklog=50

#-------------------------------------------------------------------------------
# Security Configuration
# CRITICAL: Default to PLAIN (non-SSL) on port 5556
# SSL requires additional certificate setup
#-------------------------------------------------------------------------------
SecureListener={{NODEMANAGER_SECURE_LISTENER}}
AuthenticationEnabled=true

#-------------------------------------------------------------------------------
# Startup Configuration
#-------------------------------------------------------------------------------
StartScriptEnabled=true
StartScriptName=startWebLogic.sh
StopScriptEnabled=false
NativeVersionEnabled=true
DomainsFileEnabled=true

#-------------------------------------------------------------------------------
# Logging Configuration
#-------------------------------------------------------------------------------
LogLevel=INFO
LogLimit=0
LogCount=1
LogAppend=true
LogToStderr=true
LogFormatter=weblogic.nodemanager.server.LogFormatter

#-------------------------------------------------------------------------------
# Operational Settings
#-------------------------------------------------------------------------------
StateCheckInterval=500
CrashRecoveryEnabled=false
QuitEnabled=false

#-------------------------------------------------------------------------------
# Version Information
#-------------------------------------------------------------------------------
PropertiesVersion=14.1.1.0.0
